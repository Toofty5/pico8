function check_guess()
	s=score_guess(guesses[#guesses])
	if s[1] == #answer then
		win=true
	end
end 

function score_guess(g)
	local ans_dict={}
	local guess_dict={}
	for i=1,#bird_set do
		s=tostr(bird_set[i])
		ans_dict[s]=0
		guess_dict[s]=0
	end
	
	local hit=0
	local blow=0
	
	for i=1,#answer do
		local this_ans=answer[i]
		local this_guess=g[i]
		ans_dict[tostr(this_ans)] +=1
		guess_dict[tostr(this_guess)]+=1
		if this_ans==this_guess
			then hit+=1 end
		local a=ans_dict[tostr(this_guess)]
		local g=guess_dict[tostr(this_guess)]
	end
	
	for k,v in pairs(guess_dict) do
		blow += min(ans_dict[k],v)
	end
	blow -= hit
	
	return {hit,blow}
end

function map_guess(indices)
	local return_me={}
	for i in all(indices) do
		add(return_me, bird_set[i])
	end
	return return_me
end


