--bird management--

function new_ans()
	local return_me={}
	x=ceil(rnd(len_set))
	for i=1,len_ans do
		add(return_me, x)
	end
	return return_me
end

function new_set(n)
	local return_me={}
	for i=1,len_birds do
		add(return_me,i)
	end
	
	for i=1,len_birds-n do
		x=ceil(rnd(#return_me))
		del(return_me,return_me[x])
	end
	
	return return_me
end

function new_ans(this_set)
	return_me={}
	for i=1,len_ans do
		x=ceil(rnd(#this_set))
		add(return_me, this_set[x])
	end
	return return_me
end


