--draw--
function draw_birds(birds,x,y)
	for i=0,#birds-1 do
		spr(birds[i+1],x+i*9,y)
	end
end

function draw_guesses()
	for i=1,#guesses do
		local s=score_guess(guesses[i])
		local s_y=top+i*10
		local g_x=left_x+9*len_ans+1
		draw_birds(guesses[i],left_x,s_y)
		draw_score(s,g_x,s_y-1)
	end
end

function draw_cursor()		
		local x1=left_x+9*(curs-1)
		local x2=left_x+9*(curs)-1
		local g_y=top+10*(#guesses+1)

		local y1=g_y+10
		local y2=g_y+11
		line(x1,y1,x2,y1)
end

function draw_score(s,x,y)
	for i=1,s[1] do
		spr(35, x+(i-1)*5,y)
	end
	for i=1,s[2] do
		spr(36, x+(i-1)*5,y+5)
	end
	
end

function draw_ui()
	local left=91
	rect(left,0,127,127,7)
	s=[[
length

set size
]]
	print(s,left+3,10)
	
end

function draw_test()
	draw_title(16,0)
	print("test!!!",8)
end

function draw_title(x,y)
	spr(16,x,y,8,1)
end

function draw_legend(x,y)
	print("=hit   =near",x+5,y)
	spr(35,x,y)
	spr(36,x+27,y)
end
