function _init()
	len_birds=5
	sound=false
	sound_status="off"
	_p_init(4,4)
end

function _p_init(a,s)
	win=false
	lose=false

	len_ans=a
	len_set=s
	bird_set=new_set(s)
	guesses={}
	guess_ind={}
	for i=1,len_ans do
		add(guess_ind,1)
	end
	guess=map_guess(guess_ind)
	answer=new_ans(bird_set)
	left_x=64-len_ans*7
	top=7
	curs=1
	
	_update=_main
	_draw=_d_main

	set_menu()
end

function _d_main()
	cls()
	local guess_y=top+10*(#guesses+1)
	draw_guesses()
	draw_title(40,0)
	draw_legend(37,10)
	if not (win or lose) then
		draw_birds(guess,left_x,guess_y)
		draw_cursor()
	end
end


function _main()
	if win then game_win() end
	if lose then game_lose() end

	update_buttons()
end


