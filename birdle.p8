--buttons and menu--
function set_menu()
	menuitem(1,"new game:",
		function(b)
			if (b&112) >0 then
				_p_init(len_ans,len_set)
			end
		end
	)
	menuitem(2,	" -birds "..len_ans,
		function(b)
			if (b&2>0 and len_ans<8) then 
				len_ans += 1
				set_menu()
			end
			if (b&1>0 and len_ans>2) then
				len_ans -= 1
				set_menu()
			end	
			if (b&112) >0 then
				_p_init(len_ans,len_set)
			end
		end
	)
	menuitem(3,
		" -set size "..len_set,
		function(b)
			if b&2>0 and len_set<len_birds then 
				len_set += 1
				set_menu()
			end
			if (b&1>0 and len_set>1) then
				len_set -= 1	
				set_menu() 
				end
			if (b&112) >0 then
				_p_init(len_ans,len_set)
			end
		end
	)	
	menuitem(4,
		"sound "..sound_status,
		function(b)
			if b&3>0 then 
			sound=not sound end
			sound_status=sound and 
			"on" or "off"
			set_menu()
		return true
		end
	)
end

