function draw_keyboard()
	left = 5
	top = 80
	for i = 1,10 do
		letter(keys[1][i],3,
			left+i*10,top)
	end
	for i = 1,9 do
		letter(keys[2][i],3,
			left+5+i*10,top+10)
	end	
	for i = 1,7 do
		letter(keys[3][i],3,
			left+15+i*10,top+20)
	end	
end

function letter(letter,bgcol,x,y)
	rectfill(x,y,x+8,y+8, bgcol)
	print(letter,x+3,y+2, 0)
end
